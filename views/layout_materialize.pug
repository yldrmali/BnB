doctype html
html
  head
    // Import Google Icon Font
    link(href='https://fonts.googleapis.com/icon?family=Material+Icons', rel='stylesheet')
    // Import materialize.css
    link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css')
    link(rel='stylesheet', href='/stylesheets/style.css')
    // Let browser know website is optimized for mobile
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
  body
    nav(style="background-color:#eceff1")
      .nav-wrapper
        a.brand-logo(href='#!' style="color:#263238 ;padding-left:1rem") BnB         
          i.material-icons cloud
        a.sidenav-trigger(href='#', data-target='mobile-demo' style="color:#263238 ")
          i.material-icons menu
        ul.right.hide-on-med-and-down
          li
            a(href='/catalog' style="color:#263238") Main Page
          li
            a(href='/catalog/home/create' style="color:#263238") Add Home
          li
            a(href='/catalog/home/delete_update'  style="color:#263238") Delete/Update Home
    ul#mobile-demo.sidenav
      .user-view(style="height:200px")
          .background
            img(src='http://lorempixel.com/400/200/sports/1/')
      li
        a(href='/catalog' style="color:#263238") Main Page
      li
        a(href='/catalog/home/create' style="color:#263238") Add Home
      li
        a(href='/catalog/home/delete_update'  style="color:#263238") Delete/Update Home

    block content
    // Compiled and minified JavaScript
    
    script(src="https://code.jquery.com/jquery-3.5.1.js" integrity="sha256-QWo7LDvxbWT2tbbQ97B53yJnYU3WhH/C8ycbRAkjPDc=" crossorigin="anonymous")
    script(src='https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js')
    script(src='/javascripts/materialize_jquery.js')
    

    